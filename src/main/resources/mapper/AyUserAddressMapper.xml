<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ay.dao.AyUserAddressDao">
    <select id="findById" resultType="com.ay.model.AyUserAddress">
        select * from ay_user_address where id = #{id}
    </select>
    <select id="findByUserId" resultType="com.ay.model.AyUserAddress">
        select * from ay_user_address where user_id = #{userId}
    </select>
</mapper>